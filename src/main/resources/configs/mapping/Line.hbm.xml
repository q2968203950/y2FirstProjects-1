<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.accp.pojo" auto-import="true">

    <class name="Line" table="line">
        <id name="lineid">
            <column name="LINEID" sql-type="int(11)"/>
        </id>
        <property name="lineno">
            <column name="LINENO" sql-type="int(11)"/>
        </property>
        <property name="linename">
            <column name="LINENAME" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="length">
            <column name="LENGTH" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>