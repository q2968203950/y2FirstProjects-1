<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.accp.pojo" auto-import="true">

    <class name="Deliveryspot" table="deliveryspot">
        <id name="deliveryspotid">
            <column name="DELIVERYSPOTID" sql-type="int(11)"/>
        </id>
        <property name="deliveryspotno">
            <column name="DELIVERYSPOTNO" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="dname">
            <column name="DNAME" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="provinceid">
            <column name="PROVINCEID" sql-type="int(11)"/>
        </property>
        <property name="cityid">
            <column name="CITYID" sql-type="int(11)"/>
        </property>
        <property name="empid">
            <column name="EMPID" sql-type="int(11)"/>
        </property>
        <property name="tel">
            <column name="TEL" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="startscope">
            <column name="STARTSCOPE" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
        <property name="startprice">
            <column name="STARTPRICE" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
        <property name="secondprice">
            <column name="SECONDPRICE" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
        <property name="flag">
            <column name="FLAG" sql-type="int(11)"/>
        </property>
        <property name="startvolumescope">
            <column name="STARTVOLUMESCOPE" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
        <property name="startvolumeprice">
            <column name="STARTVOLUMEPRICE" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
        <property name="secondvdlumeprice">
            <column name="SECONDVDLUMEPRICE" sql-type="decimal(9,2)" precision="9" scale="2"/>
        </property>
    </class>
</hibernate-mapping>