<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.accp.pojo" auto-import="true">

    <class name="Orderstate" table="orderstate">
        <id name="orderstateid">
            <column name="ORDERSTATEID" sql-type="int(11)"/>
        </id>
        <property name="oname">
            <column name="ONAME" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>