<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.accp.pojo" auto-import="true">

    <class name="Linetaile" table="linetaile">
        <id name="linedetaileid">
            <column name="LINEDETAILEID" sql-type="int(11)"/>
        </id>
        <property name="lineid">
            <column name="LINEID" sql-type="int(11)"/>
        </property>
        <property name="deliveryspotid">
            <column name="DELIVERYSPOTID" sql-type="int(11)"/>
        </property>
        <property name="lineindex">
            <column name="LINEINDEX" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>