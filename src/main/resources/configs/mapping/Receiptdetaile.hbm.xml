<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.accp.pojo" auto-import="true">

    <class name="Receiptdetaile" table="receiptdetaile">
        <id name="rdid">
            <column name="RDID" sql-type="int(11)"/>
        </id>
        <property name="receiptid">
            <column name="RECEIPTID" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="orderid">
            <column name="ORDERID" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>